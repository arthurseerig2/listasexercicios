//calcular area do retangulo
#include <stdio.h>

int main (void) {
  float area, base, altura;
  printf("Escreva o valor da base: "); scanf("%f", &base); // nao esquecer do & memoria
  printf("Escreva o valor da altura "); scanf("%f", &altura); // nao esquecer do & memoria

  area = base * altura;

  printf("O valor da area do retangulo eh: %f", area); // n usar scanf pq variavel ja ta pronta

  return 0;
}
